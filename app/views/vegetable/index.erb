<html>
  <head>
    <style type="text/css">
      table,th,td
      {
      border:1px solid blue;
      }
    </style>
  </head>

  <body>
    <table>
      <tr>
	<th>图片</th>
	<th>名称</th>
	<th>最高价</th>
	<th>最低价</th>
	<th>最近走势</th>
      </tr>
      <% @quote.each { |key, value| %>
      <tr>
	<td>
	  <img src=" <%= "/vegetables/images/#{key}.jpg" %>" />
	</td>
	<td><%= key%></td>
	<% puts value %>
	<td><%= value["price_high"]%></td>
	<td><%= value["price_low"]%></td>
	<td>
	  <img src="<%= "/vegetables/shijiazhuang/qiaoxi/kline/#{key}.png" %> " />
	<td>
      </tr>
      <% } %>
    </table>
  </body>
</html>



